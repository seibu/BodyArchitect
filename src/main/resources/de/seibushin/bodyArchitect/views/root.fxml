<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="500.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.seibushin.bodyArchitect.views.Controller">
   <bottom>
      <Label text="Adds" BorderPane.alignment="CENTER" />
   </bottom>
   <top>
      <VBox alignment="CENTER" fillWidth="false" spacing="5.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="BodyArchitect" BorderPane.alignment="CENTER" />
            <HBox alignment="CENTER" spacing="5.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#showHome" text="Home" />
                  <Button mnemonicParsing="false" onAction="#showNutrition" text="Nutrition" />
                  <Button disable="true" mnemonicParsing="false" text="Workout" />
               </children>
            </HBox>
         </children>
      </VBox>
   </top>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</BorderPane>
